<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ys.crm.workbench.dao.TranDao">

    <insert id="createByClue">
        insert into tbl_tran (id,owner,money,name,expectedDate,customerId,stage,source,contactsId,createBy,createTime)values (
#{id},
#{owner},
#{money},
#{name},
#{expectedDate},
#{customerId},
#{stage},
#{source},
#{contactsId},
#{createBy},
#{createTime}
)
    </insert>
  	
</mapper>

































